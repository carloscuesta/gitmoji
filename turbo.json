{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "tscheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "build": {
      "outputs": [".next/**", "public/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "publishPackage": {
      "dependsOn": ["^lint"],
      "outputs": []
    }
  }
}
